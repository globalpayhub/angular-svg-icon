{
  "name": "svg-icon",
  "description": "Angular 21 component and service for inlining SVGs allowing them to be easily styled with CSS.",
  "version": "21.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/czeckd/angular-svg-icon.git"
  },
  "author": "David Czeck",
  "license": "MIT",
  "keywords": [
    "angular",
    "svg",
    "icon"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dist": "ng build --configuration production angular-svg-icon && cp README.md LICENSE ./dist/angular-svg-icon/",
